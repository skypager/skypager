{
  "name": "@skypager/portfolio",
  "private": true,
  "version": "1.0.0",
  "description": "The Skypager Framework Monorepo",
  "main": "index.js",
  "resolutions": {
    "eslint-scope": "3.7.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "watch": "node scripts/watch.js",
    "prettify": "node scripts/prettify.js",
    "new": "skypager create",
    "link:devDependencies": "node scripts/link-dev-dependencies.js",
    "console": "node scripts/console.js",
    "release": "lerna publish patch --yes",
    "prerelease": "yarn test",
    "postrelease": "skypager update-main-package && cd src/main && npm version patch --skip-git && npm publish",
    "test": "lerna run test --stream",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "Skypager"
  ],
  "author": "Jon Soeder <jon@chicago.com>",
  "license": "MIT",
  "workspaces": [
    "src/devtools/*",
    "src/runtime",
    "src/runtimes/*",
    "src/features/*",
    "src/helpers/*",
    "src/servers/*",
    "src/examples/*",
    "src/main"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@skypager/node": "^0.0.16",
    "lerna": "^2.1.4",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.0.3"
  }
}
