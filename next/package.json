{
  "name": "@soederpop/luca",
  "version": "0.0.1",
  "description": "lightweight universal conversational architecture",
  "author": "jon soeder <jon@chicago.com>",
  "type": "module",
  "repository": "https://github.com/soederpop/luca",
  "license": "MIT",
  "keywords": [
    "conversational",
    "architecture"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "bin": {
    "luca": "./dist/esbuild/cli.js"
  },
  "scripts": {
    "start": "ts-node --esm scripts/start.ts",
    "dev": "ts-node --esm scripts/dev.ts",
    "console": "ts-node --esm scripts/console.ts",
    "cli": "ts-node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build:css": "tailwindcss -i src/story-gen/app/styles/main.css -o public/styles.css",
    "build:css:watch": "yarn build:css --watch",
    "build-all": "yarn clean && yarn build && yarn esbuild-node && yarn esbuild-browser",
    "esbuild-app": "ts-node --esm scripts/build.ts",
    "esbuild-browser": "esbuild src/browser.ts --bundle --minify --sourcemap=external --outfile=dist/esbuild/browser.min.js",
    "esbuild-browser:dev": "esbuild src/browser.ts --bundle --outfile=dist/esbuild/browser.js",
    "esbuild-browser:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/esbuild/browser.js",
    "esbuild-node": "esbuild src/node.ts --bundle --platform=node --packages=external --sourcemap=external --outfile=dist/esbuild/node.js",
    "esbuild-node:dev": "esbuild src/node.ts --bundle --platform=node --packages=external --sourcemap=external --outfile=dist/esbuild/node.js",
    "esbuild-node:watch": "esbuild src/node.ts --bundle --platform=node --packages=external --watch --sourcemap=external --outfile=dist/esbuild/node.js"
  },
  "devDependencies": {
    "@types/cacache": "^15.0.1",
    "@types/chalk": "^2.2.0",
    "@types/child-process-promise": "^2.2.2",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.13",
    "@types/detect-port": "^1.3.2",
    "@types/express": "^4.17.17",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/micromatch": "^4.0.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.11",
    "@types/node-uuid": "^0.0.29",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.2.0",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "esbuild": "^0.17.18",
    "esbuild-plugin-wasm": "^1.0.0",
    "eslint": "^8.14.0",
    "jest": "27.0.0",
    "prettier": "^2.8.7",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.18.0",
    "@blueprintjs/icons": "^4.15.0",
    "@blueprintjs/select": "^4.9.14",
    "@monaco-editor/react": "^4.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/body-parser": "^1.19.2",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "cacache": "^17.0.7",
    "chalk": "^5.2.0",
    "child-process-promise": "^2.2.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "detect-port": "^1.5.1",
    "express": "^4.18.2",
    "figlet": "^1.6.0",
    "inflect": "^0.5.0",
    "inquirer": "^9.1.5",
    "isomorphic-vm": "^0.0.1",
    "isomorphic-ws": "^5.0.0",
    "js-yaml": "^4.1.0",
    "langchain": "^0.0.59",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5",
    "minimist": "^1.2.8",
    "monaco-editor": "^0.38.0",
    "node-uuid": "^1.4.8",
    "object-hash": "^3.0.0",
    "opener": "^1.5.2",
    "postcss": "^8.4.23",
    "puppeteer": "^19.11.1",
    "puppeteer-har": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-mosaic-component": "^6.0.1",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "ws": "^8.13.0"
  }
}
