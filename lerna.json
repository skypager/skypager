{
  "lerna": "3.4.0",
  "registry": "https://registry.npmjs.org/",
  "packages": [
    "src/apps/*",
    "src/clients/*",
    "src/devtools/*",
    "src/examples/*",
    "src/features/*",
    "src/helpers/*",
    "src/runtime",
    "src/runtimes/*",
    "src/servers/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "develop",
        "master",
        "next"
      ],
      "ignoreChanges": [
        "**/*.md",
        "**/*.spec.js"
      ]
    },
    "run": {
      "npmClient": "yarn"
    }
  },
  "version": "1.4.2"
}
